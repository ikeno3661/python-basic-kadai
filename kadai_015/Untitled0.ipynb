        "class Human:\n",
        "  # コンストラクタを定義する\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(\"name: \" + self.name)\n",
        "    print(\"age: \" + str(self.age))\n",
        "\n",
        "sr = Human(\"tanaka\", 20)\n",
        "sr.printinfo()"
      ]
